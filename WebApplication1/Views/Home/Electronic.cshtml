@{
    ViewBag.Title = "Elektronik";

    AllInOneEntities db = new AllInOneEntities();
    List<Urun> urunler = db.Urun.Where(x => x.urunID > 1 && x.urunID < 13).ToList();
    List<UrunResmi> resimler = db.UrunResmi.Where(x => x.urunID > 1 && x.urunID < 53).ToList();

}

<div class="container">
    <div class="row">
        <div class="col-md-10">
                <div class="col-md-4 productCard">
                    @foreach (var x in urunler)
                    {
                        <div class="productImage">
                            <div class="productMainImage">
                                <img src="@(x.UrunResmi.First().Url.url1)" class="resim-wrap" id="product" alt="">
                            </div>
                            <div class="productImages">
                                @foreach (var item in x.UrunResmi)
                                {


                                    <img src="@(item.Url.url1)" onclick="change_img(this);" id="" alt="">
                                }
                            </div>
                        </div>
                        <div class="productInformation">
                            <div class="productName">@(x.adi)</div>
                            <div class="productDescription">@(x.aciklama)@(x.urunKodu)</div>
                            <div class="productPrice">@(x.fiyat) TL</div>
                            <a href="#" class="productBuy">Sepete Ekle</a>
                        </div>
                     }
                </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    var container;   
    function change_img(image) {
        container.src = image.src;
    }
</script>